<!DOCTYPE html>
<html>
  <head>
    <title>RevistaFestaJa</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag "https://cdnjs.cloudflare.com/ajax/libs/jquery.mask/1.14.0/jquery.mask.min.js" %>
  </head>

  <body>    
    <header>
      <div class="logo">
        
      </div>
      <div class="dados-user text-right">
        <i class='glyphicon glyphicon-user'></i>
        <%= current_user.email %>
        <%= link_to raw("<i class='glyphicon glyphicon-off'></i>"), destroy_user_session_path, method: :delete %>
      </div>
    </header>
    <div id="admin-modal" class="modal fade"></div>
    <div class="menu">
      <%= render 'administrador/menu' %>      
    </div>
    
    <div class="principal" id='principal-adm'>
      <%= yield %>      
      
    </div>
    

    
  </body>
</html>
